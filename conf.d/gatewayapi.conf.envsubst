location = /network {
    auth_basic off;
    proxy_pass ${DOLLAR}clientAPI;
    include conf.d/enable-websocket.conf;
    include conf.d/enable-cors.conf;
}

location ~ ^/(token|account|validator|validators|transaction) {
    auth_basic off;
    proxy_pass ${DOLLAR}clientAPI;
    include conf.d/enable-websocket.conf;
    include conf.d/enable-cors.conf;
}
